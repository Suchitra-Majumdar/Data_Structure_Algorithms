
def max_sum_subarray(arr,k):
    i = 0
    j = 0
    ans = 0
    n = len(arr)
    for i in range(n-k+1):
        sum = 0
        for j in range(i,i+k):
            sum+=arr[j]           
        ans = max(ans,sum)
                
       
    return ans

print(max_sum_subarray([1,-1,2,3,-2,4,5,6],3))